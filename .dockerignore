# Docker ignore file for aqua-watch-ml
# Ignore git files
.git
.gitignore
.gitattributes

# Ignore IDE and editor files
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# Ignore Python cache and compiled files
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
build/
develop-eggs/
dist/
downloads/
eggs/
.eggs/
lib/
lib64/
parts/
sdist/
var/
wheels/
*.egg-info/
.installed.cfg
*.egg
MANIFEST

# Ignore virtual environments
venv/
env/
ENV/
env.bak/
venv.bak/
.venv/

# Ignore environment files (should be provided via docker-compose or secrets)
.env
.env.local
.env.development
.env.test
.env.production

# Ignore logs (will be created in container)
logs/
*.log

# Ignore data cache (will be created in container)
data/cache/
*.cache

# Ignore model files if they're large (can be mounted or downloaded)
# Uncomment if models are too large to include in image
# models/saved_models/
# *.joblib
# *.pkl
# *.h5

# Ignore test files and coverage reports
.pytest_cache/
.coverage
.tox/
htmlcov/
.nox/
coverage.xml
*.cover
.hypothesis/

# Ignore documentation
docs/
*.md
README*

# Ignore Docker files (they're already being used)
Dockerfile*
docker-compose*.yml
.dockerignore

# Ignore CI/CD files
.github/
.gitlab-ci.yml
.travis.yml
Jenkinsfile

# Ignore temporary files
tmp/
temp/
*.tmp
*.temp

# Ignore OS specific files
Thumbs.db
ehthumbs.db
Desktop.ini

# Ignore backup files
*.bak
*.backup
